<div class = "user_index_container">
  <% @users.each do |user| %>
    <div  class = "user_profile">
      <div> <%= image_tag "img/profile.jpeg" %>  </div>
        <div class = "borders">
          <h3 class= "user_name"><%= user.name %></h3>
          <span class= "num">Number of posts: <%= user.post_counter %></span>
        </div>
    
    </div>
  <% end %>
</div>
